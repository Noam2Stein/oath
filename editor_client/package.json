{
  "name": "oath",
  "displayName": "oath",
  "description": "Oath language support for VsCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "oath",
      "aliases": ["oath", "oath"],
      "extensions": ["oath"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "oath",
      "scopeName": "source.oath",
      "path": "./syntaxes/oath.tmLanguage.json"
    }]
  }
}
